{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"target": "es2018",
		"module": "esnext",
		"lib": ["esnext"],
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"strict": true,
		"strictNullChecks": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"skipDefaultLibCheck": true
	}
}
